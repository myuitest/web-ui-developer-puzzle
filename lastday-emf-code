{"output":{"value":"false"}}
{"output":{"value":"false"}}
{"output":{"value":"false"}}
{"output":{"value":"false"}}
eval_expression = function(json_row, str_eval)
{
    row = JSON.parse(json_row);
    str = str_eval;
    for (key in row) {
        str = str.replace(key, row[key])
    }
    try {
        return {"value": eval(str), "err": null}
    } catch (err) {
        return {"value": null, "err": str_eval + ' with params ' + json_row + ' : ' + err.message}
    }
