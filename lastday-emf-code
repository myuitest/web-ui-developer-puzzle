      """{"bucket_cfs":"[$bucket_cfs]","file_path_input":"[$file_path_input]"}""".stripMargin
    val sourceProcessTasks: Seq[ProcessTaskData] = List(
      ProcessTaskData("T01", "SPARK-INGEST", List.empty, sparkIngestParameters, testTopic, "E31"))

