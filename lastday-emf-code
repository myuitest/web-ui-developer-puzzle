private[cashflows] object ReferenceCurve extends RowToJSCollection[ReferenceCurve] {
  override def apply(v1: Row): ReferenceCurve = {
    ReferenceCurve(
      v1.getAs[java.sql.Date]("date"),
      v1.getAs[Double]("rate")
    )
  }
