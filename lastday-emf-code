        |			, VARIABLE_AMOUNT: null
        |			, VARIABLE_RATE: null
        |			, VARIABLE_STEP: null
        |		}]
        |	}
        |}
        |
        |if (typeof module !== 'undefined') {
        |	module.exports = {
        |		processCashflows,
        |		eoMonthUTC,
        |		toDateComponents,
        |		eoMonthUTC,
