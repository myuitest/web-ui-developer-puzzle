        |// @flow strict
        |
        |/*::
        |type CashFlowDateType = "L" | "F" |"A";
        |type AmortisationType = "MODIFIED RULE OF 78"| "ACCRUAL" | "BULLET" | "STRAIGHT LINE" | "LEVEL PAY";
        |type InsFreqTimeUnit = "D" | "W" | "M" | "Q" | "Y" | "AM" | "" | "H";
