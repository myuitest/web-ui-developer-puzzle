      case "!=" => NotEqual
      case ">" => GreaterThan
      case ">=" => GreaterThanOrEqual
      case "<" => LessThan
      case "<=" => LessThanOrEqual
      case "IN" => In
      case "NOT IN" => NotIn
      case "LIKE" => Like
      case "NOT LIKE" => NotLike
      case "IS" => Is
      case "IS NOT" => IsNot
    }
    case JNull => null
  },
  {
    case comparisonOperator:ComparisonOperator => JString(comparisonOperator.getClass.getSimpleName.replace("$",""))
  }))
