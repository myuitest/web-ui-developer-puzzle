     |        p = 1.0052589676941592334 + p * w;
     |        p = 3.0838856104922207635 + p * w;
     |    } else if (isFinite(w)) {
     |        w = Math.sqrt(w) - 5.0;
     |        p = -2.7109920616438573243e-11;
     |        p = -2.5556418169965252055e-10 + p * w;
     |        p = 1.5076572693500548083e-09 + p * w;
     |        p = -3.7894654401267369937e-09 + p * w;
